{
    "version": 2,
    "builds": [
        {
            "src": "dist/server.js",
            "use": "@vercel/node",
            "config": {
                "includeFiles": [
                    "dist/**"
                ]
            }
        }
    ],
    "routes": [
        {
            "src": "/api/(.*)",
            "dest": "dist/server.js"
        },
        {
            "src": "/(.*)",
            "dest": "dist/server.js"
        }
    ],
    "env": {
        "NODE_ENV": "production"
    }
}